{
  "rust-analyzer.inlayHints.enable": false,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "clangd.semanticHighlighting": true,
  "cSpell.userWords": ["INET", "Unorderable", "funcs"],
  "python.formatting.provider": "black",
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "css",
    "php",
    "rust",
    "markdown",
    "python",
    "vue",
    "scss",
    "jinja",
    "html",
    "c"
  ],
  "emmet.includeLanguages": { "jinja": "html" },
  "clangd.semanticHighlighting": true,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "vim",
    "latex",
    "yaml",
    "yml"
  ],
  "cSpell.userWords": ["INET", "Unorderable", "funcs"],
  "suggest": {
    "autoTrigger": "always",
    "enablePreview": true,
    "minTriggerInputLength": 2
  },
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  }
}
